target 'Demo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Demo
  pod 'Alamofire', '~> 4.7'
  pod 'SDWebImage', '~> 4.0'
  pod 'ModelMapper'


  target 'DemoTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'ModelMapper'

  end

  target 'DemoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
